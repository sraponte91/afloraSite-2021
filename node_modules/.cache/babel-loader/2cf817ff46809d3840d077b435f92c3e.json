AA,KAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,SAAA,GAAA,6BAAA;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,KAAA;AACA,UAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,SAPA,MAOA;AACA,UAAA,KAAA,CAAA,SAAA,GAAA,8CAAA;AACA,UAAA,KAAA,CAAA,SAAA,GAAA,KAAA;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,IAAA;AACA;AAEA,OAjBA,EAiBA,KAjBA,CAiBA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,8CAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,KAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,IAAA;AACA,OAtBA;AAuBA,KAlCA;AAmCA,IAAA,OAnCA,qBAmCA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,KAAA;AACA,WAAA,SAAA,GAAA,KAAA;AACA;AAvCA;AAnBA,CAAA","sourcesContent":["<template>\n    <section id=\"contact\" class=\"contact-us-section ptb-100\" v-bind:class=\"{ 'gray-light-bg' : isGray }\">\n        <div class=\"container\">\n            <div class=\"row justify-content-around\">\n                <div class=\"col-12 pb-3 message-box\"\n                     v-bind:class=\"{ 'd-none': !isSuccess && !hasError, 'd-block': isSuccess || hasError }\">\n                    <div class=\"alert\" v-bind:class=\"{ 'alert-danger': hasError, 'alert-success': isSuccess}\">{{ alertText }}</div>\n                </div>\n                <div class=\"col-md-12 col-lg-5 mb-5 mb-md-5 mb-sm-5 mb-lg-0\">\n                    <div class=\"contact-us-form gray-light-bg rounded p-5\">\n                        <h4>Ready to get started?</h4>\n                        <form v-on:submit=\"submit\" id=\"contactForm\" class=\"contact-us-form\">\n                            <div class=\"form-row\">\n                                <div class=\"col-12\">\n                                    <div class=\"form-group\">\n                                        <input\n                                            type=\"text\"\n                                            class=\"form-control\"\n                                            name=\"name\"\n                                            placeholder=\"Enter name\"\n                                            required=\"required\"\n                                            v-model=\"name\"\n                                        />\n                                    </div>\n                                </div>\n                                <div class=\"col-12\">\n                                    <div class=\"form-group\">\n                                        <input\n                                            type=\"email\"\n                                            class=\"form-control\"\n                                            name=\"email\"\n                                            placeholder=\"Enter email\"\n                                            required=\"required\"\n                                            v-model=\"email\"\n                                        />\n                                    </div>\n                                </div>\n                                <div class=\"col-12\">\n                                    <div class=\"form-group\">\n                                        <textarea\n                                            name=\"message\"\n                                            id=\"message\"\n                                            class=\"form-control\"\n                                            rows=\"7\"\n                                            cols=\"25\"\n                                            placeholder=\"Message\"\n                                            v-model=\"message\"\n                                        ></textarea>\n                                    </div>\n                                </div>\n                                <div class=\"col-sm-12 mt-3\">\n                                    <button type=\"submit\" class=\"btn btn-brand-02\" id=\"btnContactUs\">Send Message</button>\n                                </div>\n                            </div>\n                        </form>\n                    </div>\n                </div>\n                <div class=\"col-md-12 col-lg-6\">\n                    <div class=\"contact-us-content\">\n                        <h2>Looking for a excellent Business idea?</h2>\n                        <p  class=\"lead\">Give us a call or drop by anytime, we endeavour to answer all enquiries within 24 hours on business days.</p>\n                        <a href=\"#\" class=\"btn btn-outline-brand-01 align-items-center\">\n                            Get Directions\n                            <span class=\"ti-arrow-right pl-2\"></span>\n                        </a>\n                        <hr class=\"my-5\" />\n                        <ul class=\"contact-info-list\">\n                            <li class=\"d-flex pb-3\">\n                                <div class=\"contact-icon mr-3\">\n                                    <span class=\"fas fa-location-arrow color-primary rounded-circle p-3\"></span>\n                                </div>\n                                <div class=\"contact-text\">\n                                    <h5 class=\"mb-1\">Company Location</h5>\n                                    <p>100 Yellow House, Mn Factory, United State, 13420</p>\n                                </div>\n                            </li>\n                            <li class=\"d-flex pb-3\">\n                                <div class=\"contact-icon mr-3\">\n                                    <span class=\"fas fa-envelope color-primary rounded-circle p-3\"></span>\n                                </div>\n                                <div class=\"contact-text\">\n                                    <h5 class=\"mb-1\">Email Address</h5>\n                                    <p>hello@yourdomain.com</p>\n                                </div>\n                            </li>\n                        </ul>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </section>\n</template>\n\n\n<script>\n    export default {\n        name: 'Contact',\n        props: {\n            isGray: {\n                type: Boolean,\n                default: false\n            }\n        },\n        data(){\n            return {\n                email: '',\n                message: '',\n                name: '',\n                alertText: '',\n                hasError: false,\n                isSuccess: false\n            };\n\n        },\n        methods: {\n            submit: function(e) {\n\n                e.preventDefault();\n\n\n                let formData = new FormData();\n                formData.append('name', this.name);\n                formData.append('message', this.message);\n                formData.append('email', this.email);\n\n                fetch(\"/php/contact-form-process.php\", {\n                        body: formData,\n                        method: \"POST\"\n                }).then(response => {\n                    if (response.status === 200) {\n                        this.name = '';\n                        this.message = '';\n                        this.email = '';\n                        this.alertText = 'Form submitted successfully';\n                        this.hasError = false;\n                        this.isSuccess = true;\n                    } else {\n                        this.alertText = 'Found error in the form. Please check again.';\n                        this.isSuccess = false;\n                        this.hasError = true;\n                    }\n\n                }).catch(error => {\n                    console.log(error);\n                    this.alertText = 'Found error in the form. Please check again.';\n                    this.isSuccess = false;\n                    this.hasError = true;\n                });\n            },\n            mounted() {\n                this.alertText = '';\n                this.hasError = false;\n                this.isSuccess = false;\n            }\n        }\n    }\n</script>\n\n"],"sourceRoot":"src/views/index-one"}]}